<mvc:View
    controllerName="com.app.project1.controller.View1"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    height="100%"
>
 
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader id="_IDGenToolHeader1">
                <Image
                    id="_IDGenImage1"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://www.sap.com/dam/application/shared/logos/sap-logo-svg.svg"
                />
                <Title
                    level="H1"
                    text="Parking Lot Dashboard"
                    wrapping="false"
                    id="Guardsdashboard"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData1"
                            priority="Disappear"
                        />
                    </layoutData>
                </Title>
                <!-- <Text
                    text="Second Title"
                    wrapping="false"
                    id="secondTitle"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData2"
                            priority="Disappear"
                        />
                    </layoutData>
                </Text> -->
                <!-- <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                <SearchField
                    width="16rem"
                    id="searchField"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData3"
                            priority="Low"
                            group="1"
                        />
                    </layoutData>
                </SearchField> -->
               
                <!-- <Button
                    visible="false"
                    icon="sap-icon://search"
                    type="Transparent"
                    id="searchButton"
                    tooltip="Search"
                /> -->
                
                <!-- <OverflowToolbarButton
                    id="_IDGenOverflowToolbarButton2"
                    icon="sap-icon://card"
                    type="Transparent"
                    tooltip="Action 2"
                    text="Action 2"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData6"
                            group="2"
                        />
                    </layoutData>
                </OverflowToolbarButton> -->
                <!-- <ToolbarSeparator id="_IDGenToolbarSeparator1">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData7"
                            group="2"
                        />
                    </layoutData>
                </ToolbarSeparator> -->
                
                <Button
                    id="_IDGenButton1"
                    icon="sap-icon://bell"
                    type="Transparent"
                    tooltip="Notification"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData8"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Button>
                <tnt:ToolHeaderUtilitySeparator
                    id="_IDGenToolHeaderUtilitySeparator1"
                />
                <ToolbarSpacer
                    id="_IDGenToolbarSpacer2"
                    width="1.125rem"
                />
                
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData9"
                            priority="NeverOverflow"
                        />
                    </layoutData>
              
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:subHeader>
            <tnt:ToolHeader id="_IDGenToolHeader2">
               <IconTabHeader
						selectedKey="{/selectedKey}"
						items="{path: '/navigation'}"
						select=".onItemSelect"
						mode="Inline">
					<layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData10"
                            priority="NeverOverflow"
                            shrinkable="true"
                        />
                    </layoutData>
                    <items>
                        <IconTabFilter
                            id="_IDGenIconTabFilter9"
                            items="{items}"
                            text="{title}"
                            key="{key}"
                            interactionMode="SelectLeavesOnly"
                        >
                            <items>
                                <IconTabFilter
                                    id="_IDGenIconTabFilter2"
                                    text="{title}"
                                    key="{key}"
                                />
                            </items>
                        </IconTabFilter>
                    </items>
                </IconTabHeader>
            </tnt:ToolHeader>
        </tnt:subHeader>
        <tnt:mainContents>
            <NavContainer
                id="pageContainer"
                initialPage="page1"
            >
                <pages>
                    <ScrollContainer
                        id="page1"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                        
                    >
                        <Table
                            id="table"
                            items="{/PlotNOs}"
                                mode="MultiSelect"
                              
                        >
                            <columns>
                                <Column>
                                    <Text text="plot number" />
                                </Column>
                                <Column>
                                    <Text text="Type" />
                                </Column>
                                <Column>
                                    <Text text="Availability" />
                                </Column>
                                <Column>
                                    <Text text="vehical" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <Text text="{plot_NO}" />
                                        
                                    </cells>
                                    <cells>
                                        <Text text="{inBoundOroutBound}" />
                                        
                                    </cells>
                                    <cells>
                                        <Text text="{available}" />
                                        
                                    </cells>
                                    <cells>
                                        <Text text="" />
                                        
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </ScrollContainer>
                    <ScrollContainer
                        id="page2"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                         <Table
                            id="table1"
                            items="{tableModel>/items}"
                        >
                            <columns>
                                <Column>
                                    <Text text="Parking Lot Number" />
                                </Column>
                                <Column>
                                    <Text text="Type" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <Text text="{tableModel>name}" />
                                        <Text text="{tableModel>description}" />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </ScrollContainer>
                    <!-- <ScrollContainer
                        id="page3"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText3"
                            text="Users and Groups"
                        />
                    </ScrollContainer> -->
                    <ScrollContainer
                        id="page4"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                    <Table
                            id="table4"
                            items="{tableModel>/items}"
                        >
                            <columns>
                                <Column>
                                    <Text text="Vehicle Number" />
                                </Column>
                                <Column>
                                    <Text text="Driver Name" />
                                </Column>
                                <Column>
                                    <Text text="Driver Mobile Number" />
                                </Column>
                                 <Column>
                                    <Text text="Parking lot number" />
                                </Column>
                                <Column>
                                    <Text text="Assigned on" />
                                </Column>
                                <Column>
                                    <Text text="Process Type" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <Text text="{tableModel>name}" />
                                        <Text text="{tableModel>description}" />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                       
                    </ScrollContainer>
                    <ScrollContainer
                        id="page5"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                                <FlexBox
                            justifyContent="Center"
                            alignItems="Center"
                            height="50%"
                        >
                            <VBox width="30%">
                                <VBox
                                    width="30%"
                                    alignItems="Center"
                                >
                                    
                                </VBox>
 
                                <VBox width="100%">
                                    <VBox width="100%">
                                        <Label text="Driver Name" />
                                        <Input
                                            id="driverNameInput"
                                            placeholder="Enter driver's name"
                                            width="100%"
                                        />
                                    </VBox>
 
                                    <VBox width="100%">
                                        <Label text="Driver Phone Number" />
                                        <Input
                                            id="driverPhoneInput"
                                            placeholder="Enter driver's phone number"
                                            width="100%"
                                            type="Tel"
                                        />
                                    </VBox>
 
                                    <VBox width="100%">
                                        <Label text="Vehicle Type" />
                                        <Select
                                            id="vehicleTypeSelect"
                                            width="100%"
                                        >
                                           
                                        </Select>
                                    </VBox>
 
                                    <VBox width="100%">
                                        <Label text="Parking Lot Number" />
                                        <Input
                                            id="parkingLotInput"
                                            placeholder="Enter parking lot number"
                                            width="100%"
                                        />
                                    </VBox>
 
                                    <VBox width="100%">
                                        <Label text="Inward or Outward" />
                                        <RadioButtonGroup
                                            id="inOutRadioGroup"
                                            columns="2"
                                            width="100%"
                                        >
                                            <buttons>
                                                <RadioButton
                                                    text="Inward"
                                                    selected="true"
                                                />
                                                <RadioButton text="Outward" />
                                            </buttons>
                                        </RadioButtonGroup>
                                    </VBox>
 
                                    <VBox width="100%">
                                        <Label text="Date and Time" />
                                        <DateTimePicker
                                            id="dateTimePicker"
                                            width="100%"
                                            value="{path: '/date'}"
                                            displayFormat="yyyy/MM/dd HH:mm:ss"
                                        />
                                    </VBox>
 
                                    <VBox width="100%">
                                        <Label text="Comments" />
                                        <TextArea
                                            id="commentsTextArea"
                                            placeholder="Enter any additional comments"
                                            width="100%"
                                            height="100px"
                                        />
                                    </VBox>
 
                                    <HBox
                                        justifyContent="End"
                                        width="100%"
                                        alignItems="Center"
                                    >
                                        <Button
                                            text="Assign"
                                            press="onAssignPress"
                                            width="120px"
                                            type="Success"
                                        />
                                        <Button
                                            text="Unassign"
                                            press="onUnassignPress"
                                            width="120px"
                                            type="Reject"
                                        />
                                    </HBox>
                                </VBox>
                            </VBox>
                        </FlexBox>
                    </ScrollContainer>
                    <ScrollContainer
                        id="page6"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText6"
                            text="Reservations"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="page7"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText7"
                            text="Vendor Book Slot"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="page8"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText8"
                            text="Charts"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="page9"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText9"
                            text="History"
                        />
                    </ScrollContainer>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>