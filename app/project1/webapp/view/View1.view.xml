<mvc:View
    controllerName="com.app.project1.controller.View1"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    height="100%"
>
      <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader id="_IDGenToolHeader1">
                <Image
                    id="_IDGenImage1"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://www.sap.com/dam/application/shared/logos/sap-logo-svg.svg"
                />
                <Title
                    level="H1"
                    text="Parking Lot Application"
                    wrapping="false"
                    id="productName"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData1"
                            priority="Disappear"
                        />
                    </layoutData>
                </Title>
                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                <ToolbarSpacer id="spacer">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData4"
                            priority="Low"
                            group="1"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Button
                    visible="false"
                    icon="sap-icon://search"
                    type="Transparent"
                    id="searchButton"
                    tooltip="Search"
                />
                <OverflowToolbarButton
                    id="_IDGenOverflowToolbarButton1"
                    icon="sap-icon://print"
                    type="Success"
                    tooltip="Print"
                    text="Print"
                    visible="false"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData5"
                            group="2"
                        />
                    </layoutData>
                </OverflowToolbarButton>
                
                <ToolbarSeparator id="_IDGenToolbarSeparator1">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData7"
                            group="2"
                        />
                    </layoutData>
                </ToolbarSeparator>
                <!-- <OverflowToolbarButton
                    id="_IDGenOverflowToolbarButton3"
                    icon="sap-icon://action-settings"
                    type="Transparent"
                    text="Settings"
                /> -->
                <!-- <Button
                    id="_IDGenButton1"
                    icon="sap-icon://bell"
                    type="Transparent"
                    tooltip="Notification"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData8"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Button> -->
                <tnt:ToolHeaderUtilitySeparator
                    id="_IDGenToolHeaderUtilitySeparator1"
                />
                <ToolbarSpacer
                    id="_IDGenToolbarSpacer2"
                    width="1.125rem"
                />
                <Avatar
                    id="_IDGenAvatar1"
                    src="test-resources/sap/tnt/images/Woman_avatar_01.png"
                    displaySize="XS"
                    press=".onAvatarPressed"
                    tooltip="Profile"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData9"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Avatar>
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:subHeader>
            <tnt:ToolHeader id="_IDGenToolHeader2">
                <IconTabHeader
                    id="_IDGenIconTabHeader1"
                    selectedKey="{/selectedKey}"
                    items="{path: '/navigation'}"
                    select=".onItemSelect"
                    mode="Inline"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData10"
                            priority="NeverOverflow"
                            shrinkable="true"
                        />
                    </layoutData>
                    <items>
                        <IconTabFilter
                            id="_IDGenIconTabFilter1"
                            items="{items}"
                            text="{title}"
                            key="{key}"
                            icon="{icon}"
                            interactionMode="SelectLeavesOnly"
                        >
                            <items>
                                <IconTabFilter
                                    id="_IDGenIconTabFilter2"
                                    text="{title}"
                                    key="{key}"
                                />
                            </items>
                        </IconTabFilter>
                    </items>
                </IconTabHeader>
            </tnt:ToolHeader>
        </tnt:subHeader>
        <tnt:mainContents>
            <NavContainer
                id="pageContainer"
                initialPage="page1"
            >
                <pages>
                    <ScrollContainer
                        id="page1"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="classpage1"
                    >
                        <FlexBox
                            id="idflex"
                            justifyContent="Center"
                            alignItems="Center"
                            height="50%"
                        >
                            <VBox
                                id="_IDGenVBox1"
                                width="30%"
                            >
                                <VBox
                                    id="_IDGenVBox2"
                                    width="30%"
                                    alignItems="Center"
                                >
                                    <Image
                                        id="_IDGenImage2"
                                        src="path_to_your_image"
                                        width="80%"
                                        height="auto"
                                    />
                                </VBox>

                                <VBox
                                    id="_IDGenVBox3"
                                    width="100%"
                                >
                                    <VBox
                                        id="_IDGenVBox10"
                                        width="100%"
                                    >
                                        <Label
                                            id="_IDGenLabel7"
                                            text="Vehicel Number"
                                            class="bold-red-label"
                                        />
                                        <Input
                                            id="commentsTextArea"
                                            placeholder="Enter Vehicel Number"
                                            value="{localModel>/VehicalDeatils/vehicalNo}"
                                            tooltip="Format: AB 12 CD 3456,AB12CD3456"
                                        />
                                    </VBox>

                                    <VBox
                                        id="_IDGenVBox4"
                                        width="100%"
                                    >
                                        <Label
                                            id="_IDGenLabel1"
                                            text="Driver Name"
                                            class="bold-red-label"
                                        />
                                        <Input
                                            id="driverNameInput"
                                            placeholder="Enter driver's name"
                                            width="100%"
                                            value="{localModel>/VehicalDeatils/driverName}"
                                             tooltip="Enter driver's name (at least 3 letters, no special characters or numbers)"
                                        />
                                    </VBox>

                                    <VBox
                                        id="_IDGenVBox5"
                                        width="100%"
                                    >
                                        <Label
                                            id="_IDGenLabel2"
                                            text="Driver Phone Number"
                                            class="bold-red-label"
                                        />
                                        <Input
                                            id="driverPhoneInput"
                                            placeholder="Enter driver's phone number"
                                            width="100%"
                                            type="Tel"
                                            value="{localModel>/VehicalDeatils/phone}"
                                            tooltip="Enter driver's phone number (10 digits)"
                                            
                                        />
                                    </VBox>

                                    <VBox
                                        id="_IDGenVBox6"
                                        width="100%"
                                    >
                                        <Label
                                            id="_IDGenLabel3"
                                            text="Vehicle Type"
                                            class="bold-red-label"
                                        />
                                        <Input
                                            id="idvehiceltype"
                                            placeholder="Enter vehicel type"
                                            width="100%"
                                            value="{localModel>/VehicalDeatils/vehicalType}"
                                        />
                                    </VBox>

                                    <VBox
                                        id="_IDGenVBox7"
                                        width="100%"
                                    >
                                        <Label
                                            id="_IDGenLabel4"
                                            text="Parking Lot Number"
                                            class="bold-red-label"
                                        />
                                        <Input
                                            id="productInput"
                                            placeholder="Select ParkingLot"
                                            showSuggestion="true"
                                            showValueHelp="true"
                                            visible="{ path: 'available'}"
                                            valueHelpRequest=".onValueHelpRequest"
                                            suggestionItems="{
                                                 path: '/PlotNOs',
                                                 sorter: {
                                                            path: 'plot_NO',
                                                            descending: false
                                                          },
                                                    filters: [
                                                          {
                                                            path: 'available',
                                                            operator: 'EQ',
                                                            value1: true
                                                          }
                                                              ]
                                                        }"
                                            suggestionItemSelected=".onSuggestionItemSelected"
                                            value="{localModel>/plotNo_plot_NO}"
                                        >
                                            <suggestionItems>
                                                <core:ListItem
                                                    id="idlistitem1"
                                                    key="{plot_NO}"
                                                    text="{plot_NO}"
                                                    additionalText="{plot_NO}"
                                                />
                                            </suggestionItems>
                                        </Input>
                                    </VBox>

                                    <VBox
                                        id="_IDGenVBox8"
                                        width="100%"
                                    >
                                        <Label
                                            visible="false"
                                            id="_IDGenLabel5"
                                            text="Inward or Outward"
                                            class="bold-red-label"
                                        />
                                        <Input
                                            visible="false"
                                            id="idinputinout"
                                            placeholder="Inward/outward"
                                            width="100%"
                                        />
                                    </VBox>

                                    <HBox
                                        id="_IDGenHBox1"
                                        justifyContent="End"
                                        width="100%"
                                        alignItems="Center"
                                    >
                                        
                                        <VBox
                                            id="idvboxemptyspace"
                                            width="10%"
                                        />
                                        
                                        <VBox
                                            id="idvboxemptyspace2"
                                            width="10%"
                                        />
                                        <VBox id="idbuttonvboxassign">
                                            <Button
                                                id="idbuttonassign"
                                                text="Assign"
                                                press="onAssignPress"
                                                width="100px"
                                                type="Success"
                                            />
                                        </VBox>
                                    </HBox>
                                </VBox>
                            </VBox>
                        </FlexBox>
                    </ScrollContainer>

                    <ScrollContainer
                        id="page2"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Table
                            id="AllSlotsTable"
                            class="tableclass"
                            alternateRowColors="true"
                            growing="true"
                            width="100%"
                            growingThreshold="5"
                            items="{/PlotNOs}"
                        >
                            <headerToolbar>
                                <OverflowToolbar id="idSlotsTableHeadertable2">
                                    <Title
                                        id="idTableHeaderTitletable2"
                                        text="All Parking Slots"
                                    />
                                    <ToolbarSpacer
                                        id="idTableHeaderSpacertable2"
                                    />
                                </OverflowToolbar>
                            </headerToolbar>
                            <columns>
                                <Column id="idcolslotno">
                                    <Text
                                        id="idtextslotno"
                                        text="Slot Number"
                                    />
                                </Column>
                                <Column id="idcolparkintype">
                                    <Text
                                        id="idtextparkintype"
                                        text="Parking Type"
                                    />
                                </Column>
                                <Column id="idcolstatus">
                                    <Text
                                        id="idtextstatus"
                                        text="Status"
                                    />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem
                                    id="AllslotsCLI"
                                    class="classtable2"
                                >
                                    <cells>
                                        <Text
                                            id="idtextitemsno"
                                            text="{plot_NO}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="idtextitemstype"
                                            text="{inBoundOroutBound}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="idProcessvalue"
                                            text="{
                                               path: 'available',
                                                formatter:'.statusTextFormatter'
                                            }"
                                        />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </ScrollContainer>
                    <ScrollContainer
                        id="page3"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Table
                            id="AssignedSlotsTable"
                            class="classassigned"
                            alternateRowColors="true"
                            mode="MultiSelect"
                            growing="true"
                            growingThreshold="6"
                            items="{/VehicalDeatils}"
                        >
                            <headerToolbar>
                                <OverflowToolbar id="idSlotsTableHeader">
                                    <Title
                                        id="idTableHeaderTitle"
                                        text="Assigned Slots"
                                    />
                                    <ToolbarSpacer id="idTableHeaderSpacer" />
                                    <Button
                                        id="idprintbutton"
                                        icon="sap-icon://print"
                                        type="Success"
                                        tooltip="Print"
                                        text="Print"
                                        press="OnPrintpress"
                                    />
                                    <Button text="Multiple Unassign " press=".onUnassignAllPress"  type="Success"/>
                                </OverflowToolbar>
                       
                            </headerToolbar>
                            <columns>
                                <Column id="idcolvehicleno">
                                    <Text
                                        id="_IDGenText8"
                                        text="Vehicle Number"
                                    />
                                </Column>
                                <Column id="idcoldrivername">
                                    <Text
                                        id="_IDGenText9"
                                        text="Driver Name"
                                    />
                                </Column>
                                <Column id="idcoldriverph">
                                    <Text
                                        id="_IDGenText10"
                                        text="Driver Ph Number"
                                    />
                                </Column>
                                <Column id="idcolvehicletype">
                                    <Text
                                        id="_IDGenText11"
                                        text="Vehicle Type"
                                    />
                                </Column>
                                <Column id="idcolparkinglot">
                                    <Text
                                        id="_IDGenText12"
                                        text="Parking Lot number"
                                    />
                                </Column>
                                <Column id="idcomlparkinglot">
                                    <Text
                                        id="_IDGenTextdate"
                                        text="Entry Date"
                                    />
                                </Column>
                                
            <Column>
                <Text text="Unassign"/>
               
           </Column>
                            </columns>
                            <items>
                                <ColumnListItem
                                    id="Assignedslots"
                                    class="classassigned"
                                >
                                    <cells>
                                        <Text
                                            id="_IDGenText13"
                                            text="{vehicalNo}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="_IDGenText14"
                                            text="{driverName}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="_IDGenText15"
                                            text="{phone}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="_IDGenText16"
                                            text="{vehicalType}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="_IDGenText17"
                                            text="{plotNo_plot_NO}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="_IDGenText18"
                                            text="{assignedDate}"
                                            
                                        />
                                    </cells>
                                    <Button text="Unassign" press=".onUnassignPress" 
                                     type= "Success" />
                                </ColumnListItem>
                            </items>

                        </Table>
                        
                    </ScrollContainer>
                    <ScrollContainer
                        id="page4"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Table
                            id="EmptySlotsTable"
                            class="classParkinglot"
                            alternateRowColors="true"
                            growing="true"
                            width="100%"
                            growingThreshold="6"
                            items="{/PlotNOs}"
                        >
                            <headerToolbar>
                                <OverflowToolbar
                                    id="idSlotsTableHeaderemptable"
                                >
                                    <Title
                                        id="idTableHeaderTitleemptable"
                                        text="Empty Slots"
                                    />
                                    <ToolbarSpacer
                                        id="idTableHeaderSpaceremptable"
                                    />
                                </OverflowToolbar>
                            </headerToolbar>
                            <columns>
                                <Column id="idcolemptyslotno">
                                    <Text
                                        id="idtextsemptylotno"
                                        text="Slot Number"
                                    />
                                </Column>
                                <Column id="idcolemptyparkintype">
                                    <Text
                                        id="idtextemptyparkintype"
                                        text="Parking Type"
                                    />
                                </Column>
                                <Column id="idcolemptystatus">
                                    <Text
                                        id="idtextemptystatus"
                                        text="Status"
                                    />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem
                                    id="emptyslotsCLI"
                                    class="classemptytable"
                                    visible="{ path: 'available'}"
                                >
                                    <cells>
                                        <Text
                                            id="idtextemptyitemsno"
                                            text="{plot_NO}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="idtexemptyemstype"
                                            text="{inBoundOroutBound}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="idProcessemptyvalue"
                                            text="{ path: 'available', formatter: '.statusTextFormatter' }"
                                        />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </ScrollContainer>
                    <ScrollContainer
                        id="page5"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText5"
                            text="Provisioning"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="page6"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText6"
                            text="Monitoring"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="page7"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="classpagehistory"
                    >
                        <Table
                            id="AssignedhistoryTable"
                            class="classassigned"
                            alternateRowColors="true"
                            growing="true"
                            growingThreshold="6"
                            items="{/History}"
                        >
                            <headerToolbar>
                                <OverflowToolbar id="idhistoryTableHeader">
                                    <Title
                                        id="idTablehistoryHeaderTitle"
                                        text="Parking Slot History"
                                    />
                                    <ToolbarSpacer
                                        id="idTableHistoryHeaderSpacer"
                                    />
                                </OverflowToolbar>
                            </headerToolbar>
                            <columns>
                                <Column id="idcolvehiclenoHistory">
                                    <Text
                                        id="idtexthistoryvhno"
                                        text="Vehicle Number"
                                    />
                                </Column>
                                <Column id="idcoldrivernameHistory">
                                    <Text
                                        id="idtexthistoryname"
                                        text="Driver Name"
                                    />
                                </Column>
                                <Column id="idcoldriverphHistory">
                                    <Text
                                        id="idtexthistoryphno"
                                        text="Driver Ph Number"
                                    />
                                </Column>
                                <Column id="idcolvehicletypeHistory">
                                    <Text
                                        id="idtexthistoryvtype"
                                        text="Vehicle Type"
                                    />
                                </Column>
                                <Column id="idcolparkinglotHistory">
                                    <Text
                                        id="idtexthistoryplot"
                                        text="Parking Lot Number"
                                    />
                                </Column>
                                <Column id="idcolintime">
                                    <Text
                                        id="idtexthistoryintime"
                                        text="In Time"
                                    />
                                </Column>
                                <Column id="idcolouttime">
                                    <Text
                                        id="idtexthistoryoutime"
                                        text="Out Time"
                                    />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem
                                    id="Historyslots"
                                    class="classassigned"
                                >
                                    <cells>
                                        <Text
                                            id="cellhistoryvno"
                                            text="{vehicalNo}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="cellhistorydbname"
                                            text="{driverName}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="cellhistoryph"
                                            text="{phone}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="cellhistoryvtype"
                                            text="{vehicalType}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text text="{plotNo}" />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="cellhistoryintime"
                                            text="{assignedDate}"
                                        />
                                    </cells>
                                    <cells>
                                        <Text
                                            id="cellhistoryoutime"
                                            text="{unassignedDate}"
                                        />
                                    </cells>
                                    <!-- Add more cells for additional fields -->
                                </ColumnListItem>
                            </items>
                        </Table>
                    </ScrollContainer>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>