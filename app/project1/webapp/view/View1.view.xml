<mvc:View
    controllerName="com.app.project1.controller.View1"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    height="100%"
>
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader id="_IDGenToolHeader1">
                <Image
                    id="_IDGenImage1"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://www.sap.com/dam/application/shared/logos/sap-logo-svg.svg"
                />
                <Title
                    level="H1"
                    text="Parking Lot Dashboard"
                    wrapping="false"
                    id="Guardsdashboard"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData1"
                            priority="Disappear"
                        />
                    </layoutData>
                </Title>
                <Button
                    id="_IDGenButton1"
                    icon="sap-icon://bell"
                    type="Transparent"
                    tooltip="Notification"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData81"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Button>
                <tnt:ToolHeaderUtilitySeparator
                    id="_IDGenToolHeaderUtilitySeparator1"
                />
                <ToolbarSpacer
                    id="_IDGenToolbarSpacer2"
                    width="1.125rem"
                />
                <Text>
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData9"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Text>
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:subHeader>
            <tnt:ToolHeader id="_IDGenToolHeader2">
                <IconTabHeader
                    id="bjbhdf"
                    selectedKey="{/selectedKey}"
                    items="{path: '/navigation'}"
                    select=".onItemSelect"
                    mode="Inline"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData10"
                            priority="NeverOverflow"
                            shrinkable="true"
                        />
                    </layoutData>
                    <items>
                        <IconTabFilter
                            id="_IDGenIconTabFilter9"
                            items="{items}"
                            text="{title}"
                            key="{key}"
                            interactionMode="SelectLeavesOnly"
                        >
                            <items>
                                <IconTabFilter
                                    id="_IDGenIconTabFilter2"
                                    text="{title}"
                                    key="{key}"
                                />
                            </items>
                        </IconTabFilter>
                    </items>
                </IconTabHeader>
            </tnt:ToolHeader>
        </tnt:subHeader>
        <tnt:mainContents>
            <NavContainer
                id="pageContainer"
                initialPage="page1"
            >
                <pages>
                    <ScrollContainer
                        id="page1"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Table
                            id="table"
                            items="{/PlotNOs}"
                            
                        >
                            <columns>
                                <Column>
                                    <Text text="Plot Number" />
                                </Column>
                                <Column>
                                    <Text text="Type" />
                                </Column>
                                <Column>
                                    <Text text="Availability" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <Text text="{plot_NO}" />
                                    </cells>
                                    <cells>
                                        <Text text="{inBoundOroutBound}" />
                                    </cells>
                                    <cells>
                                        <Text text="{available}" />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </ScrollContainer>
                    <ScrollContainer
                        id="page4"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Table
                            id="table4"
                            items="{tableModel>/items}"
                            mode="MultiSelect"
                        >
                            <columns>
                                <Column>
                                    <Text text="Vehicle Number" />
                                </Column>
                                <Column>
                                    <Text text="Driver Name" />
                                </Column>
                                <Column>
                                    <Text text="Driver Mobile Number" />
                                </Column>
                                <Column>
                                    <Text text="Parking Lot Number" />
                                </Column>
                                <Column>
                                    <Text text="Assigned Date and Time" />
                                </Column>
                                 
                                <Column>
                                    <Text text="Actions" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <Text text="{tableModel>vehicalDetails_vehicalNo}" />
                                        <Text text="{tableModel>driverName}" />
                                        <Text text="{tableModel>drivernumber}" />
                                        <Text text="{tableModel>plotNo_plot_NO}" />
                                        <Text text="{tableModel>assignedDate}" />
                                        <Button text="Unassign" press="onUnassignSinglePress" />
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                        <Button text="Unassign Selected" press="onUnassignSelectedPress" type="Reject" />
                    </ScrollContainer>
                   <ScrollContainer
    id="page5"
    horizontal="false"
    vertical="true"
    height="100%"
    class="sapUiContentPadding"
>
    <FlexBox
        justifyContent="Center"
        alignItems="Center"
        height="50%"
    >
        <VBox width="30%">
            <VBox alignItems="Center" />
            <VBox width="100%">
                <Label text="Driver Name" />
                <Input
                    id="driverNameInput"
                    placeholder="Enter driver's name"
                    width="100%"
                />
            </VBox>
            <VBox width="100%">
                <Label text="Driver Phone Number" />
                <Input
                    id="driverPhoneInput"
                    placeholder="Enter driver's phone number"
                    width="100%"
                    type="Tel"
                />
            </VBox>
            <VBox width="100%">
                <Label text="Vehicle Number" />
                <Input
                    id="Vehiclenumber"
                    placeholder="Enter vehicle number"
                    width="100%"
                />
            </VBox>
             <VBox width="100%">
        <Label text="Parking Lot Number" />
        <Select
            id="parkingLotSelect"
            width="100%"
            items="{
                path: '/PlotNOs',
            templateShareable: false,
            filters: [{ path: 'available', operator: 'EQ', value1: 'Available' }]
        }"
    >
        <core:Item
            key="{plot_NO}"
            text="{plot_NO}"
        />
    </Select>

    </VBox>
            <VBox width="100%">
                <Label text="Inward or Outward" />
                <RadioButtonGroup
                    id="inOutRadioGroup"
                    columns="2"
                    width="100%"
                >
                    <buttons>
                        <RadioButton text="Inward" selected="true" />
                        <RadioButton text="Outward" />
                    </buttons>
                </RadioButtonGroup>
            </VBox>
            <VBox width="100%">
                <Label text="Date and Time" />
                <DateTimePicker
                    id="dateTimePicker"
                    width="100%"
                    value="{path: '/date'}"
                    displayFormat="yyyy/MM/dd HH:mm:ss"
                />
            </VBox>
            <HBox
                justifyContent="End"
                width="100%"
                alignItems="Center"
            >
                <Button
                    text="Assign"
                    press="onAssignPress"
                    width="120px"
                    type="Success"
                />
            </HBox>
        </VBox>
    </FlexBox>
</ScrollContainer>
                    <ScrollContainer
                        id="page6"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText6"
                            text="Reservations"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="page7"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText7"
                            text="Vendor Book Slot"
                        />
                    </ScrollContainer>
                    <ScrollContainer
                        id="page8"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text
                            id="_IDGenText8"
                            text="Charts"
                        />
                    </ScrollContainer>
                    <ScrollContainer
    id="page9"
    horizontal="false"
    vertical="true"
    height="100%"
    class="sapUiContentPadding"
>
    <Text text="History" />
    <Table id="unassignedTable" items="{unassignedModel>/unassignedItems}">
        <columns>
            <Column>
                <Text text="Vehicle Number" />
            </Column>
            <Column>
                <Text text="Driver Name" />
            </Column>
            <Column>
                <Text text="Driver Mobile Number" />
            </Column>
            <Column>
                <Text text="Parking Lot Number" />
            </Column>
            <Column>
                <Text text="Assigned Date and Time" />
            </Column>
            <Column>
                <Text text="Unassigned Date and Time" />
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{unassignedModel>vehicalDetails_vehicalNo}" />
                    <Text text="{unassignedModel>driverName}" />
                    <Text text="{unassignedModel>drivernumber}" />
                    <Text text="{unassignedModel>plotNo_plot_NO}" />
                    <Text text="{unassignedModel>assignedDate}" />
                    <Text text="{unassignedModel>unassignedDate}" />
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</ScrollContainer>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
